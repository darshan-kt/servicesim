<%
  # Large office furniture template
  #
  # Required variables
  # * $o (number) length of an office unit
  # * $pi (number)
%>

<include>
  <pose>
    <%= -1.6*$o %>
    -0.4
    0
    0
    0
    0
  </pose>
  <uri>model://mini_fridge</uri>
</include>

<%
  $pose =
  [
    2 * $o,
    -3 * $o,
    0,
    0,
    0,
    -$pi * 0.5,
  ]
%>
<%= fromFile(DIR + "/cubicle_corner.erb") %>

