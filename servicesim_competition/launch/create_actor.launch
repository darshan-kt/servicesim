<launch>
  <arg name="world_name" value="$(find servicesim_competition)/worlds/create_actor.world"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <env name="GAZEBO_RESOURCE_PATH" value="$(find servicesim_competition)/worlds"/>
    <env name="GAZEBO_MODEL_PATH" value="$(find servicesim_competition)/models"/>
    <arg name="world_name" value="$(arg world_name)" />
    <arg name="paused" value="false"/>
    <arg name="gui" value="true"/>
    <arg name="verbose" value="true"/>
  </include>
</launch>
